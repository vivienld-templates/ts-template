{
  "name": "template",
  "version": "1.0.0",
  "description": "A typescript project template ",
  "main": "src/index.ts",
  "scripts": {
    "commit":"cz",
    "test": "npx jest --collectCoverage",
    "build:test": "jest-test-gen ./src/%FILE% --outputDir ../test --fileSuffix .test",
    "build:package": "tsc",
    "build:doc": "npx typedoc src/index.ts",
    "build": "npm run generate:doc && npm run generate:package"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "jest": "^29.5.0",
    "jest-test-gen": "^1.4.3",
    "ts-jest": "^29.1.0",
    "typedoc": "^0.24.1",
    "typescript": "^5.0.4"
  }
}
