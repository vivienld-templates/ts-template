{
  "name": "template",
  "version": "1.0.0",
  "description": "A typescript project template ",
  "main": "build/index.js",
  "scripts": {
    "test": "npx jest --collectCoverage",
    "build:test": "jest-test-gen ./src/%FILE% --outputDir ../test --fileSuffix .test",
    "build:package": "tsc",
    "build:docs": "npx typedoc src/index.ts",
    "build": "npm run build:docs && npm run build:package"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/pino": "^7.0.5",
    "@types/pino-pretty": "^5.0.0",
    "jest": "^29.5.0",
    "jest-test-gen": "^1.4.3",
    "pino": "^8.11.0",
    "pino-pretty": "^10.0.0",
    "ts-jest": "^29.1.0",
    "typedoc": "^0.24.1",
    "typescript": "^5.0.4"
  }
}
